<% include ./partials/_header %>
<h1>
  <%=selectedTag || 'Tags'%>
</h1>
<% tags.forEach(tag => { %>
<div class="form-check-inline">
  <% const linkClass = (tag._id == selectedTag)?'badge badge-warning': 'badge badge-primary'%>
  <a href="/tags/<%=tag._id%>" class="<%=linkClass%>">
    <label>
      <%=tag._id%>
      <%=tag.count%>
    </label>
  </a>
</div>
<% })%>
</body>

</html>